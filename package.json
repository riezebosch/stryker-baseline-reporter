{
  "name": "stryker-baseline-reporter",
  "version": "0.0.1",
  "description": "Reporting newly discovered mutants for Stryker",
  "main": "src/Index.js",
  "scripts": {
    "lint": "tslint -p .",
    "prepublishOnly": "npm run build",
    "build": "tsc -p .",
    "postbuild": "npm run lint",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "stryker": "stryker run stryker.conf.js",
    "start": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riezebosch/stryker-baseline-reporter.git"
  },
  "keywords": [
    "stryker",
    "stryker-plugin",
    "stryker-reporter",
    "mutation",
    "testing",
    "baseline"
  ],
  "author": "Manuel Riezebosch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/riezebosch/stryker-baseline-reporter/issues"
  },
  "homepage": "https://github.com/riezebosch/stryker-baseline-reporter#readme",
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/lodash": "^4.14.66",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "stryker": "^0.6.4",
    "stryker-api": "^0.5.3",
    "stryker-mocha-framework": "^0.1.2",
    "stryker-mocha-runner": "^0.4.2",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "stryker-api": "^0.5.3"
  }
