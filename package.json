{
  "name": "stryker-baseline-reporter",
  "version": "1.0.3",
  "description": "Report improvement (or retrogression) on your mutation score with Stryker",
  "main": "src/Index.js",
  "scripts": {
    "lint": "tslint -p .",
    "prepublishOnly": "npm run build",
    "build": "tsc -p .",
    "postbuild": "npm run lint",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "stryker": "stryker run stryker.conf.js",
    "start": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riezebosch/stryker-baseline-reporter.git"
  },
  "keywords": [
    "stryker",
    "stryker-plugin",
    "stryker-reporter",
    "mutation",
    "testing",
    "baseline"
  ],
  "author": "Manuel Riezebosch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/riezebosch/stryker-baseline-reporter/issues"
  },
  "homepage": "https://github.com/riezebosch/stryker-baseline-reporter#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/lodash": "^4.14.91",
    "@types/mocha": "^2.2.45",
    "@types/mz": "0.0.32",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "stryker": "^0.35.1",
    "stryker-api": "^0.12.0",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "stryker-typescript": "^0.18.1",
    "tslib": "^1.8.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "stryker-api": ">= 0.9 < 1"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "lodash": "^4.17.4",
    "mz": "^2.7.0",
    "tempy": "^0.2.1"
  }
}
